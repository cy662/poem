{
  "name": "vue-poem-app",
  "version": "1.0.0",
  "description": "一个基于Vue 3的诗词管理应用",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "pinia": "^2.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/tsconfig": "^0.5.0",
    "typescript": "^5.3.0",
    "vue-tsc": "^1.8.0",
    "vite": "^5.0.0",
    "@types/node": "^20.10.0"
  },
  "keywords": [
    "vue3",
    "typescript",
    "pinia",
    "vue-router",
    "poem",
    "poetry"
  ],
  "author": "Vue Developer",
  "license": "MIT"
